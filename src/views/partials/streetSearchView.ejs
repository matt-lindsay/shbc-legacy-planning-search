<div id="streetSelector">
    <form name="streetSearchForm" action="/Street/name" method="get">
        <div class="form-group">
            <label for="name">Please enter a street name within Surrey Heath</label>
            <input type="text" id="name" name="name" class="form-control" />
        </div>
        <div class="form-group">
            <input type="submit" value="Search"/>
        </div>
    </form>
</div>